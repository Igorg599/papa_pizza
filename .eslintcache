}) => drink.isLoaded);\r\n    React.useEffect(() => {\r\n      dispatch(fetchDrink());\r\n    }, []);\r\n\r\n    const handleAddDrinkToCart = (obj) => {\r\n      dispatch({\r\n        type: 'ADD_PIZZA_CART',\r\n        payload: obj\r\n      });\r\n    }\r\n\r\n  \r\n    return (\r\n      <div className=\"container\">\r\n          <div className=\"content__items\">\r\n            { isLoaded ?\r\n              items.map(obj => \r\n              <DrinkBlock \r\n              onClickAddDrink={handleAddDrinkToCart} \r\n              key={obj.id}\r\n              addedCount={cartItems[obj.id] && cartItems[obj.id].items.length} \r\n              {...obj}/>)\r\n              : Array(2).fill(0).map((_, index) => <LoadingBlock key={index}/>)\r\n            }\r\n          </div>\r\n      </div>\r\n    )\r\n  }\r\n  \r\n  export default Drink;","C:\\Users\\user\\Desktop\\Гордеев\\11\\PAPAPIZZA\\pizza\\src\\components\\Header.jsx",["196"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport Categories from './Categories';\r\nimport {useSelector, useDispatch} from 'react-redux';\r\nimport {setCategory} from '../redux/action/filters';\r\nimport { useEffect } from \"react\";\r\n\r\nimport logoSvg from '../assets/img/pizza-logo.jpg';\r\nimport pizza from '../assets/img/pizza.gif';\r\n\r\nconst categoryNames = [{name: 'Пицца', path: '/'}, {name: 'Комбо', path: '/combo'}, {name: 'Напитки', path: '/drink'}, {name: 'Горячие закуски', path: '/snacks' }, {name: 'Соусы к пицце', path: '/sauce'}, {name: 'Пицца за час!', path: '/hour'}];\r\n\r\nfunction Header() {\r\n    const {totalPrice, totalCount} = useSelector(({cart}) => ({\r\n        totalPrice: cart.totalPrice,\r\n        totalCount: cart.totalCount,\r\n    }));\r\n\r\n    const dispatch = useDispatch();\r\n    const {category} = useSelector(({filters}) => filters);\r\n\r\n    const onSelectCategory = React.useCallback((index) => {\r\n        dispatch(setCategory(index));\r\n    }, []);\r\n\r\n    useEffect(() => {\r\n        const header = document.getElementById(\"myHeader\");\r\n        const logo = document.getElementById(\"logo\");\r\n\r\n        const sticky = header.offsetTop;\r\n        const scrollCallBack = window.addEventListener(\"scroll\", () => {\r\n            if (window.pageYOffset >= sticky + 50) {\r\n            header.classList.add(\"sticky\");\r\n            logo.style.transition = '1s';\r\n            logo.style.display = 'block';\r\n            } else {\r\n            header.classList.remove(\"sticky\");\r\n            logo.style.display = 'none';\r\n            }\r\n        });\r\n        return () => {\r\n        window.removeEventListener(\"scroll\", scrollCallBack);\r\n        };\r\n    }, []);\r\n\r\n    return (\r\n        <div className=\"header\">\r\n            <div className=\"container\">\r\n                <Link to=\"/\">\r\n                    <div className=\"header__logo\">\r\n                        <img width=\"150\" src={logoSvg} alt=\"Pizza logo\" />\r\n                    </div>   \r\n                </Link>\r\n            </div>\r\n            <div className=\"container\">\r\n                <div id=\"myHeader\" className=\"header__fixed\">\r\n                    <img id=\"logo\" className=\"logo_mini\" width=\"100\" src={logoSvg} alt=\"logo\"></img>\r\n                    <Categories activeCategory={category} onClickCategory={onSelectCategory} items={categoryNames}/>\r\n                    <div  className=\"header__cart\">\r\n                        <Link to=\"/cart\">\r\n                            <button onClick={() => onSelectCategory(88)} className=\"button outline\">\r\n                                <a href=\"/cart.html\" className=\"button button--cart\">\r\n                                    <span>{totalPrice} ₽</span>\r\n                                    <div className=\"button__delimiter\"></div>\r\n                                    <svg className=\"header__cart_svg\"\r\n                                   